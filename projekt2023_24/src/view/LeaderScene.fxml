<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          maxHeight="600.0" maxWidth="600.0" minHeight="400.0" minWidth="400.0"
          alignment="center"
          hgap="5"
          vgap="5"
          stylesheets="@style.css">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <ImageView fitHeight="119.0" fitWidth="116.0" pickOnBounds="true" preserveRatio="true" GridPane.valignment="TOP">
        <image>
            <Image url="@logo.png" />
        </image>
        <GridPane.margin>
            <Insets />
        </GridPane.margin>
    </ImageView>

    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#leaderButtonAction" styleClass="logoutButton" text="Log out" textAlignment="CENTER" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.valignment="TOP">
        <GridPane.margin>
            <Insets right="10.0" top="10.0" />
        </GridPane.margin>
    </Button>

    <!-- Dodane Label'e -->
    <Label fx:id="name" styleClass="textInfo" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
        <GridPane.margin>
            <Insets bottom="10.0" />
        </GridPane.margin></Label>
    <Label fx:id="lastName" styleClass="textInfo" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
        <GridPane.margin>
            <Insets bottom="10.0" />
        </GridPane.margin></Label>
    <Label fx:id="role" styleClass="textInfo" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
        <GridPane.margin>
            <Insets bottom="10.0" />
        </GridPane.margin></Label>
    <Label fx:id="zone" styleClass="textInfo" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
        <GridPane.margin>
            <Insets bottom="10.0" />
        </GridPane.margin></Label>

    <TabPane fx:id="tabPane" GridPane.columnIndex="0" GridPane.columnSpan="5" GridPane.rowIndex="2">
        <tabs>

        </tabs>
        <GridPane.margin>
            <Insets left="20.0" right="20.0" top="10.0" />
        </GridPane.margin>
    </TabPane>

    <columnConstraints>
        <ColumnConstraints maxWidth="172.0000183105469" minWidth="13.5" prefWidth="200.0" />
        <ColumnConstraints maxWidth="314.0" minWidth="0.0" prefWidth="200.0" />
        <ColumnConstraints maxWidth="342.0" minWidth="0.0" prefWidth="200.0" />
        <ColumnConstraints hgrow="ALWAYS" maxWidth="415.0" minWidth="56.0" prefWidth="200.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="191.0" minHeight="10.0" prefHeight="120.40000610351564" vgrow="ALWAYS" />
        <RowConstraints maxHeight="445.60000915527337" minHeight="47.20001831054688" prefHeight="54.399993896484375" vgrow="ALWAYS" />
        <RowConstraints maxHeight="445.60000915527337" minHeight="169.0" prefHeight="384.80000000000007" vgrow="ALWAYS" />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
</GridPane>
